<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EarphoneLeftAndRight.Views.DictionaryTabbed"
            xmlns:v="clr-namespace:EarphoneLeftAndRight.Views"
            xmlns:r="clr-namespace:EarphoneLeftAndRight.Resx"
            >
  <!--Pages can be added as references or inline-->
    <v:DictionaryPage Title="{x:Static r:AppResources.Dict_Left_Title}" Html="{x:Static r:AppResources.Dict_Left_Html}" DictionaryName="{x:Static r:AppResources.Dict_Dict_Title}"/>
    <v:DictionaryPage Title="{x:Static r:AppResources.Dict_Right_Title}" Html="{x:Static r:AppResources.Dict_Right_Html}" DictionaryName="{x:Static r:AppResources.Dict_Dict_Title}"/>
</TabbedPage>